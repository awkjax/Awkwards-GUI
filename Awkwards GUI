local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Awkward's GUI", "Midnight")

-- Gui to Lua

local Farming = Window:NewTab("Farming")
local FarmingSection = Farming:NewSection("Farming")

FarmingSection:NewButton("IOS Farm", "Clicking on this button executes a farm for IOS", function()
  loadstring(game:HttpGet('https://raw.githubusercontent.com/PRODUPERR/BS2BestFarm/main/BS2Farm'))()
end)

FarmingSection:NewButton("Dupe Script", "Clicking on this button executes a dupe script", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/BaconBossScript/Crazy/main/Crazy'),true))()()
end)


local Performance = Window:NewTab("Performance")
local PerformanceSection = Performance:NewSection("Performance")

PerformanceSection:NewButton("Performance Script", "Clicking on this button executes a Performance Script", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/FyPvDnNw"))()
end)

local Misc = Window:NewTab("Misc")
local MiscSection = Misc:NewSection("Misc")

MiscSection:NewButton("Loop Script", "Clicking on this button executes a script which has a loop in it", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/AliceZurburg/CloudyHub/main/loader"))()
end)
